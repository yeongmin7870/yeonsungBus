<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/design_default_color_primary</item>
        <item name="colorPrimaryDark">@color/design_default_color_primary_dark</item>
        <item name="colorAccent">@color/design_default_color_on_primary</item>
    </style>
    <style name="expandedBarTextStyle" parent="AppTheme">
        <!-- Primary brand color. -->
        <item name="android:fontFamily">@font/welcomeotfregular</item>
        <item name="android:textSize">13dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:theme">@string/dashboard_scroll_after</item>
        
    </style>
    
    <style name="circleImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">15%</item>
    </style>

    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">5dp</item>
        <item name="background">@color/white</item>
    </style>
</resources>